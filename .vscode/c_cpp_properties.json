{
    "configurations": [
        {
            "name": "UCRT64",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/include/**",
                "C:/vcpkg/installed/x64-mingw-dynamic/include",
                "C:/msys64/ucrt64/include/**"
            ],
            "defines": [
                "_WIN32_WINNT=0x0A00",
                "WINVER=0x0A00",
                "UNICODE",
                "_UNICODE",
                "BOOST_ALL_NO_LIB",
                "_DEBUG"
            ],
            "compilerPath": "C:/msys64/ucrt64/bin/g++.exe",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "gcc-x64",
            "compilerArgs": [
                "-std=c++20",
                "-Wall",
                "-Wextra",
                "-I${workspaceFolder}/include",
                "-I${workspaceFolder}/src"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "C:/vcpkg/installed/x64-mingw-dynamic/include",
                    "C:/msys64/ucrt64/include"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}